{
  "private": true,
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "homepage": "https://lumirlumir.github.io/web-moing.lumir.page",
  "repository": "https://github.com/lumirlumir/web-moing.lumir.page",
  "scripts": {
    "prepare": "husky",
    "start": "npx webpack serve --mode=development --open",
    "build": "npx webpack build --mode=production --config ./webpack.config.js",
    "deploy": "gh-pages -d build",
    "fix": "concurrently \"npm:fix:*\"",
    "fix:eslint": "npx eslint --fix",
    "fix:prettier": "npx prettier . --write",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:eslint": "npx eslint",
    "lint:prettier": "npx prettier . --check",
    "lint:editorconfig": "npx editorconfig-checker -config .editorconfig-checker.json",
    "lint:markdownlint": "npx markdownlint **/*.md"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "prop-types": "^15.8.1",
    "qs": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.0",
    "react-speech-recognition": "^3.10.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.1",
    "editorconfig-checker": "^6.0.0",
    "eslint": "^9.20.1",
    "eslint-config-bananass": "^0.0.4",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "lerna": "^8.1.9",
    "lint-staged": "^15.2.10",
    "markdownlint-cli": "^0.43.0",
    "prettier": "^3.5.1",
    "prettier-config-bananass": "^0.0.1",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
