{
  "private": true,
  "homepage": "https://lumirlumir.github.io/Web_Capstone",
  "repository": "https://github.com/lumirlumir/Web_Capstone",
  "scripts": {
    "start": "npx webpack serve --mode=development --open",
    "build": "npx webpack build --mode=production --config ./webpack.config.js",
    "eslint-check": "npx eslint . --ext .js,.jsx",
    "eslint-fix": "npx eslint . --fix --ext .js,.jsx",
    "prettier-check": "npx prettier . --check",
    "prettier-fix": "npx prettier . --write",
    "deploy": "gh-pages -d build",
    "git-add": "npm run eslint-fix && npm run prettier-fix && git add ."
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
