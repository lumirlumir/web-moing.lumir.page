{
  "private": true,
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*"
  ],
  "engines": {
    "node": ">=20.19.4"
  },
  "homepage": "https://lumirlumir.github.io/web-moing.lumir.page",
  "repository": "https://github.com/lumirlumir/web-moing.lumir.page",
  "scripts": {
    "prepare": "husky",
    "dev": "node ./webpack.config.mjs dev",
    "build": "node ./webpack.config.mjs build",
    "deploy": "gh-pages -d build",
    "fix": "concurrently \"npm:fix:*\"",
    "fix:eslint": "npx eslint --fix",
    "fix:prettier": "npx prettier . --write --ignore-unknown",
    "fix:markdownlint": "npx markdownlint **/*.md --fix",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:eslint": "npx eslint",
    "lint:prettier": "npx prettier . --check --ignore-unknown",
    "lint:editorconfig": "npx editorconfig-checker -config .editorconfig-checker.json",
    "lint:markdownlint": "npx markdownlint **/*.md",
    "lint:todo": "npx tsc --noEmit && npm run test -w apps/backend"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-speech-recognition": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "editorconfig-checker": "^6.1.0",
    "eslint": "^9.35.0",
    "eslint-config-bananass": "^0.4.1",
    "eslint-plugin-mark": "^0.1.0-canary.7",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "prettier-config-bananass": "^0.4.1",
    "sass": "^1.92.1",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-dev-server": "^5.2.2"
  }
}
